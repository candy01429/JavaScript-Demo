// 變數宣告 // 使用JSFiddle測試
var x;
x =100;
var y =100;

var name1='Jim',name2='Tom';
var fruit1,fruit2;

// null與undefined
var a = null;
console.log(a); // null

var b;
console.log(b); // undefined 

var score = 100;
console.log(typeof score); // number型態

score = 'Jim';
console.log(typeof score); // String型態



